<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ISCAS.OneBeltOneRoad.dao.GisDataDao">
    <select id="queryBrAnnotationData" resultType="com.ISCAS.OneBeltOneRoad.entity.br.BrAnnotationData">
        SELECT "id", "name", "layerFeatures", "time", "style", "chart", "keyPoint", "layers"
        FROM public.br_annotationdata
        where name = #{name}
    </select>

</mapper>